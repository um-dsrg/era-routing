{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Path Selector",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/debug/pathSelector",
            "args": [
                "-i",
                "/media/noel/Data/Results/Dissertation/PathSelectorComparison/Graphs/graph1.lgf",
                "-o",
                "/media/noel/Data/Results/Dissertation/PathSelectorComparison/NewPaths/Ksp/Flowset1/ksp_1.xml",
                "--pathSelectionAlgorithm",
                "KSP",
                "--globalK",
                "5"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
