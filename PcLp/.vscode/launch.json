{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) PcLp",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/pclp",
            "args": [
                "--input=/home/noel/Documents/Results/Testing/PcLpTesting/paths.xml",
                "--output=/home/noel/Documents/Results/Testing/PcLpTesting/maxFlowOnly.xml",
                "--optimisationProblem=MaxFlowOnly"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
